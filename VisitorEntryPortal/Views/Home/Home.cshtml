        <li><a data-toggle="tab" href="#menu3" @*onclick="LoadPartialView('InterviewCandidate')"*@>Interview Candidate</a></li>
        <li><a data-toggle="tab" href="#menu4" @*onclick="LoadPartialView('Vendor')"*@>Vendor</a></li>
        <li><a data-toggle="tab" href="#menu5" @*onclick="LoadPartialView('Visitor')"*@>Visitor</a></li>


    </ul>


    <div class="tab-content">
        <div id="home" class="tab-pane fade in active">
            @{Html.RenderPartial("_LateLogout");}
